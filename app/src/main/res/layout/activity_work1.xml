<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#272626"
    tools:context=".Work1Activity">


    <LinearLayout
        android:layout_width="405dp"
        android:layout_height="806dp"
        android:background="#EE000000"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="338dp"
            android:layout_height="42dp"
            android:layout_gravity="center"
            android:layout_marginTop="70sp"
            android:gravity="center_horizontal"
            android:text="Add Your Work History"
            android:textColor="#FAFAF2"
            android:textSize="30sp"
            android:textStyle="bold|italic"
            app:layout_constraintBottom_toTopOf="@id/hr"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.078" />

        <TableRow
            android:id="@+id/hr"
            android:layout_width="363dp"
            android:layout_height="5dp"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:background="#FAF7F7"
            app:layout_constraintBottom_toTopOf="@+id/editText5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:layout_constraintVertical_bias="0.18"></TableRow>

        <EditText
            android:id="@+id/editText5"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="70dp"
            android:backgroundTint="@android:color/white"
            android:ems="10"
            android:gravity="center"
            android:hint="Company Name"
            android:inputType="textPersonName"
            android:textColor="#FAFAF5F5"
            android:textColorHint="#A4F8F4F4"
            app:layout_constraintBottom_toTopOf="@id/editText6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hr"
            app:layout_constraintVertical_bias="0.15" />

        <EditText
            android:id="@+id/editText8"
            android:layout_width="291dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="24dp"
            android:backgroundTint="@android:color/white"
            android:ems="10"
            android:gravity="center"
            android:hint="Role"
            android:inputType="textPersonName"
            android:textColor="#FAFAF8F8"
            android:textColorHint="#9EFFFFFF"
            app:layout_constraintBottom_toTopOf="@id/editText6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText5"
            app:layout_constraintVertical_bias="0.027" />

        <EditText
            android:id="@+id/editText6"
            android:layout_width="291dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="24dp"
            android:backgroundTint="@android:color/white"
            android:ems="10"
            android:gravity="center"
            android:hint="From Date"
            android:inputType="date"
            android:textColor="#FFFCFC"
            android:textColorHint="#A8F8F4F4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText8"
            app:layout_constraintVertical_bias="0.038" />

        <EditText
            android:id="@+id/editText7"
            android:layout_width="291dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="24dp"
            android:backgroundTint="@android:color/white"
            android:ems="10"
            android:gravity="center"
            android:hint="To Date"
            android:inputType="date"
            android:textColor="#F5FFFFFF"
            android:textColorHint="#ADF8F4F4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText6"
            app:layout_constraintVertical_bias="0.115" />

        <EditText
            android:id="@+id/editText9"
            android:layout_width="334dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="24dp"
            android:backgroundTint="@android:color/white"
            android:ems="10"
            android:gravity="start|top"
            android:hint="Position of Responsibility"
            android:inputType="textMultiLine"
            android:paddingLeft="60sp"
            android:paddingTop="20sp"
            android:textColor="#FCFFFFFF"
            android:textColorHint="#AAF8F4F4"
            app:layout_constraintBottom_toTopOf="@+id/button2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.532"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText7"
            app:layout_constraintVertical_bias="0.552" />

        <Button
            android:id="@+id/button2"
            android:layout_width="344dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="40dp"
            android:background="@drawable/buttonsubmit"
            android:text="SAVE"
            android:textSize="18sp"
            android:textStyle="bold|italic"
            app:layout_constraintBottom_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.475"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText7"
            app:layout_constraintVertical_bias="0.58" />

        <Button
            android:id="@+id/button10"
            android:layout_width="354dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:background="@drawable/buttonsubmit"
            android:text="Add another Work experience"
            android:textSize="18sp"
            android:textStyle="bold|italic"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.461"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button2"
            app:layout_constraintVertical_bias="0.533" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>